{
	"name": "Reminder Plus for YouTube™",
	"short_name": "YouTubeReminder+",
	"version": "2.0.5",
	"manifest_version": 3,
	"description": "YouTube™ライブのリマインダーをGoogleカレンダー™に追加します。",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.youtube.com/*"
			],
			"js": [
				"jquery-3.4.1.min.js",
				"content.js"
			]
		}
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"permissions": [
		"storage",
		"identity"
	],
	"host_permissions": [
		"https://www.googleapis.com/*"
	],
	"options_ui": {
		"page": "options.html"
	},
	"oauth2": {
		"client_id": "796253270666-50agj402br2bgao64qd1vsilh3m249ta.apps.googleusercontent.com",
		"scopes": ["https://www.googleapis.com/auth/youtube.readonly"]
	}
}
