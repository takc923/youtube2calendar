{
	"name": "Reminder Plus for YouTube™",
	"short_name": "YouTubeReminder+",
	"version": "2.0.5",
	"manifest_version": 3,
	"description": "YouTube™ライブのリマインダーをGoogleカレンダー™に追加します。",
	"icons": {
		"128": "icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.youtube.com/*"
			],
			"js": [
				"jquery-3.4.1.min.js",
				"content.js"
			]
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"storage"
	],
	"host_permissions": [
		"https://www.googleapis.com/*"
	],
	"options_ui": {
		"page": "options.html"
	}
}
